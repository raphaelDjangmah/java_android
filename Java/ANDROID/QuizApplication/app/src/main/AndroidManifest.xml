        </activity>

        <activity android:name=".MainQuiz.MainQuizScreen"
            android:label="@string/app_name">

        </activity>

        <activity android:name=".AppResources.RateApp"
            android:label="Rate this App"
            >

        </activity>

    </application>

</manifest>